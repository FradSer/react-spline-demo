{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "description": "Next.js starter by Frad.",
  "repository": "https://github.com/FradSer/nextjs-starter",
  "author": "Frad LEE <fradser@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@react-three/fiber": "^7.0.26",
    "@react-three/xr": "^3.4.0",
    "@splinetool/react-spline": "^2.0.0",
    "@splinetool/runtime": "^0.9.17",
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "three": "^0.138.3"
  },
  "devDependencies": {
    "@types/node": "17.0.4",
    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.9.0",
    "eslint-config-next": "^12.1.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.1",
    "tailwindcss": "^3.0.7",
    "typescript": "4.5.4"
  },
  "prettier": {
    "parser": "typescript",
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 80,
    "semi": true,
    "overrides": [
      {
        "files": [
          "*.json"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "eslintConfig": {
    "extends": [
      "next",
      "next/core-web-vitals"
    ],
    "rules": {
      "@next/next/no-img-element": "off"
    }
  }
}
